{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "prettier"
    ],
    "rules": {
        "no-console": [
            "error",
            {
                "allow": [
                    "info",
                    "error"
                ]
            }
        ]
    },
    "plugins": [
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}